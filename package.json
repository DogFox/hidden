    "vuetify-toast-snackbar": "^0.6.1",
    "vuex": "^3.5.1"
  },
  "devDependencies": {
    "@mdi/font": "^4.9.95",
    "@types/jest": "^24.0.19",
    "@types/lodash": "^4.14.149",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
